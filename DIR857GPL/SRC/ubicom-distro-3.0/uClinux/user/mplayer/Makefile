clean:
	-$(MAKE) -C $(VER) distclean

romfs:
	$(ROMFSINST) -d $(VER)/mplayer /bin/mplayer
	$(ROMFSINST) -d $(VER)/etc/codecs.conf /etc/codecs.conf

.PHONY: all clean romfs
