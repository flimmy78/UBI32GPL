config IP8000QEMU
	bool "IP8000QEMU"
	select UBICOM32_V5
	select MMU
	select FPU
	help
		Ubicom IP8000 QEMU supports:
			8008-QEMU v1.0

config IP8000BRINGUP
	bool "IP8000BRINGUP"
	select UBICOM32_V5
	select MMU
	select FPU
	select NEW_LEDS
	select LEDS_CLASS
	select LEDS_GPIO
	select PCIEPORTBUS if PCI
	select ARCH_SUPPORTS_MSI if PCI
	select PCI_MSI if PCI
	help
		Ubicom IP8000 BringUp, supports:
			8008-0010 v1.0

config IP8100RGW
	bool "IP8100RGW"
	select UBICOM32_V5
	select MMU
	select FPU
	select UBICOM_INPUT
	select NEW_LEDS
	select LEDS_CLASS
	select LEDS_GPIO
	select SPI
	select SPI_UBICOM32_GPIO
	select VLAN_8021Q if NET
	select UBICOM_SWITCH
	select UBICOM_SWITCH_BCM539X
	select PCIEPORTBUS if PCI
	select I2C
	select I2C_GPIO
	select GPIO_PCA953X
	select MTD_UBI32_FC
	select MTD_UBI32_NAND_SPI_ER
	select UBICOM32_MULTI_FLASH
	select ARCH_SUPPORTS_MSI if PCI
	select PCI_MSI if PCI
	select PCI_DOMAINS if PCI
	help
		Ubicom IP8100 Router Gateway Eval, supports:
			8008-0010 v1.0	IP8K RGW Board

