# Mplayer makefile for Ubicom32 
VER = mplayer-svn-28529

GCCLIBDIR := $(dir $(shell ubicom32-elf-gcc $(CFLAGS) -print-libgcc-file-name))
CONF_OPTS=--with-extraincdir="$(ROOTDIR)/include:$(ROOTDIR)/uClibc/include:$(ROOTDIR)/linux-2.6.x/include" \
				--with-extralibdir="$(ROOTDIR)/uClibc/lib:$(GCCLIBDIR)$(MARCH)/:$(ROOTDIR)/lib" \

MARCH=ubicom32v4

all: $(VER)/config.mak
	$(MAKE) -C $(VER)

$(VER)/config.mak: $(VER)/configure
	find $(VER) -type f -print0 | xargs -0 touch -r $(VER)/configure
	set -e ; \
	cp codecs.conf.h $(VER)/ ;\
	cd $(VER) ; \
	CFLAGS="--spec $(ROOTDIR)/vendors/config/ubicom32nommu/spec -march=$(MARCH) \
		-Wall -I$(ROOTDIR)/include -I$(ROOTDIR)/uClibc/include -I$(ROOTDIR)/linux-2.6.x/include" \
	LDFLAGS="$(ROOTDIR)/uClibc/lib/crt1.o -lc -lgcc" \
	_march="--spec $(ROOTDIR)/vendors/config/ubicom32nommu/spec -march=$(MARCH)" \
	./configure \
		--enable-cross-compile \
		--cc=ubicom32-elf-gcc \
		--as=ubicom32-elf-as \
		--nm=ubicom32-elf-nm \
		--ar=ubicom32-elf-ar \
		--ranlib=ubicom32-elf-ranlib \
		--enable-static \
		--target=generic \
		--enable-big-endian \
		--disable-rpath \
		--disable-mencoder \
		--disable-largefiles \
		--disable-iconv \
	 	--disable-langinfo \
		--disable-lirc \
		--disable-lircc \
		--disable-vm \
		--disable-xf86keysym \
